{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {},
    "overrides": [
        {
            "files": ["**/*.spec.js", "**/*.spec.jsx"],
            "env": {
                "jest": true
            }
        }
    ]
}
